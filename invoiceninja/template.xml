<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name>invoiceninja</Name>
  <Repository>invoiceninja/invoiceninja</Repository>
  <Registry>https://hub.docker.com/r/invoiceninja/invoiceninja/</Registry>
  <GitHub>https://github.com/invoiceninja/invoiceninja/</GitHub>
  <Project>https://www.invoiceninja.org/</Project>
  <Support>https://hub.docker.com/r/invoiceninja/invoiceninja/</Support>
  <Overview>Invoice Ninja is an invoicing application built with Laravel &amp; Flutter, which makes sending invoices and receiving payments simple and easy.</Overview>
  <Category>Productivity: Tools:</Category>
  <DateInstalled>2021-07-04</DateInstalled>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Icon>https://raw.githubusercontent.com/sgraaf/Unraid-Docker-Templates/main/invoiceninja/icon.png</Icon>
  <TemplateURL>https://raw.githubusercontent.com/sgraaf/Unraid-Docker-Templates/main/invoiceninja/template.xml</TemplateURL>
  <Config Name="WebUI" Target="80" Default="80" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="APP_URL" Target="APP_URL" Default="http://localhost:80" Description="Container Variable: APP_URL" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="APP_KEY" Target="APP_KEY" Default="" Description="Container Variable: APP_KEY" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="DB_TYPE" Target="DB_TYPE" Default="mysql" Description="Container Variable: DB_TYPE" Type="Variable" Display="always" Required="true" Mask="false">mysql</Config>
  <Config Name="DB_HOST" Target="DB_HOST" Default="" Description="Container Variable: DB_HOST" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="DB_DATABASE" Target="DB_DATABASE" Default="" Description="Container Variable: DB_DATABASE" Type="Variable" Display="always" Required="true" Mask="false"/>
